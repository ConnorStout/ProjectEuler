import time

def getNextPrime(n):
    next = n[-1]
    while next:
        next+=1
        check = True
        for x in n:
            if(next%x == 0):
                check = False
                break
        if(check == True):

            return next







def getPrimes(n):
    total = 0
    originalList = list(range(1, n))
    allIndexes = []
    nextPrime = 2
    primes = []
    while(((nextPrime*nextPrime)<n)):
        allIndexes = allIndexes + originalList[(nextPrime*2)-1::nextPrime]
        primes.append(nextPrime)
        nextPrime = getNextPrime(primes)

    allIndexes = list(set(allIndexes))
    allIndexes.sort()
    for i in reversed(allIndexes):
        del originalList[i-1]
    print(originalList)
    print(sum(originalList)-1)


a = time.time()
getPrimes(2000000)
print(time.time()-a)