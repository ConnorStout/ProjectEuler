
import numpy as np
np.set_printoptions(suppress=True,
   formatter={'float_kind':'{:f}'.format})



def lattice_solver(n):
    width = 1
    ending_width = n+1
    val_arr = np.zeros(ending_width)
    val_arr[0] = val_arr[0]+1
    while(width<ending_width):
        for x in range(1,ending_width):
            val_arr[x] = val_arr[x] + val_arr[x-1]
        val_arr[width] = val_arr[width-1]*2
        width = width+1

    print(val_arr[-1])

lattice_solver(20)