import math
import time
def getnthPrime(n):
    totalArray = []
    x = 2
    #for x in range(2,1000000):
    while x:
        check = True
        for y in totalArray:
            if x%y == 0:
                check = False
                break
        if check == True:
            totalArray.append(x)
            if (totalArray.index(x) == n - 1):

                return x
        x += 1
    print("Sorry my program can't solve that in a reasonable time")


a = time.time()
print(getnthPrime(10001))
print(time.time()-a)



