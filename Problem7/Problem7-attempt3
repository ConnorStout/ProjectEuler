import time
import math
def getPrimes(n):


    prevPrimes = []
    count = 2
    while(len(prevPrimes) < n):
        check = True
        for x in prevPrimes:
            if(x > math.sqrt(count)):
                break
            if count%x == 0:
                check = False
                break

        if check == True:
            prevPrimes.append(count)

        count+=1
    return prevPrimes.pop()


a = time.time()
print(getPrimes(10001))
print((time.time()-a))